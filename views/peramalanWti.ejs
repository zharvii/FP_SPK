<!-- Content Wrapper. Contains page content -->
<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <div class="content-header">
        <div class="container">
            <div class="row mb-2">
                <div class="col-sm-6">
                    <h1 class="m-0">Peramalan Harga Minyak Mentah WTI</h1>
                </div><!-- /.col -->
                <div class="col-sm-6">
                    <ol class="breadcrumb float-sm-right">
                        <li class="breadcrumb-item"><a href="<%= data.nav[0].URL %>">
                                <%= data.nav[0].page %>
                            </a></li>
                        <li class="breadcrumb-item">
                            <%= data.nav[1].page %>

                        </li>
                    </ol>
                </div><!-- /.col -->
            </div><!-- /.row -->
        </div><!-- /.container-fluid -->
    </div>
    <!-- /.content-header -->

    <!-- Main content -->
    <div class="content">
        <div class="container">
            <div class="row">
                <div class="col-lg-6 col-4">
                    <!-- small box -->
                    <div class="small-box bg-info">
                        <div class="inner">
                            <h4>
                                $ <%= data.hargawti.harga %> / Barrel
                            </h4>

                            <p>Hari Terbaru (<%= data.hargawti.tanggal %>)</p>
                        </div>
                        <div class="icon">
                            <i class="fas fa-money-bill"></i>
                        </div>
                    </div>
                </div>
                <!-- ./col -->

                <div class="col-lg-6 col-4">
                    <!-- small box -->
                    <div class="small-box bg-primary">
                        <div class="inner">
                            <h4>
                                $ <%= data.hargaBulanDepan %> / Barrel
                            </h4>

                            <p id="nextBulan">Peramalan Harga Bulan Selanjutnya</p>
                        </div>
                        <div class="icon">
                            <i class="fas fa-money-bill"></i>
                        </div>
                    </div>
                </div>
            </div>

            <div class="row">
                <!-- Left col -->
                <section class="col-lg-12 connectedSortable">
                    <!-- Custom tabs (Charts with tabs)-->
                    <div class="card">
                        <div class="card-header">
                            <h3 class="card-title">
                                <i class="fas fa-chart-pie mr-1"></i>
                                Harga Minyak Mentah WTI
                            </h3>
                            <div class="card-tools">

                            </div>
                        </div><!-- /.card-header -->
                        <div class="card-body">
                            <div class="tab-content p-0">
                                <!-- Morris chart - Sales -->
                                <div class="chart tab-pane active" id="revenue-chart"
                                    style="position: relative; height: 400px;">
                                    <figure class="highcharts-figure">
                                        <div id="chartku1">

                                            <div id="chartWtiJam"></div>
                                        </div>

                                        <p class="highcharts-description"></p>
                                    </figure>
                                </div>
                                <!-- <div class="chart tab-pane" id="sales-chart" style="position: relative; height: 400px;">
                                    <figure class="highcharts-figure">
                                        <div id="chartku2">

                                            <div id="chartWtiHari"></div>
                                        </div>

                                        <p class="highcharts-description"></p>
                                    </figure>
                                </div> -->
                            </div>
                        </div><!-- /.card-body -->
                    </div>
                    <!-- /.card -->


                </section>
                <!-- /.Left col -->

                <!-- right col -->
            </div>

            <!-- /.row -->
        </div><!-- /.container-fluid -->
    </div>
    <!-- /.content -->
</div>
<!-- /.content-wrapper -->